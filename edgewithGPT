Get-AppxPackage -Name Microsoft.MicrosoftEdge
Invoke-WebRequest -Uri "https://go.microsoft.com/fwlink/?linkid=2069324" -OutFile "$env:USERPROFILE\Downloads\MicrosoftEdgeChromiumInstaller.exe"
Start-Process -FilePath "$env:USERPROFILE\Downloads\MicrosoftEdgeChromiumInstaller.exe" -Wait
Get-AppxPackage -Name Microsoft.MicrosoftEdge
