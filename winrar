$winrar = Get-WmiObject -Class Win32_Product | Where-Object { $_.Name -like 'WinRAR*' }

if ($winrar) {
    Write-Output "WinRAR is installed."
} else {
    Write-Output "WinRAR is not installed."
}



$winrarInstalled = $false
$winrarPath = (Get-ItemProperty -Path "HKLM:\Software\Microsoft\Windows\CurrentVersion\Uninstall\*").PSChildName | ForEach-Object {
    if ($_ -match "WinRAR") {
        $winrarInstalled = $true
        break
    }
}

if ($winrarInstalled) {
    Write-Output "WinRAR is installed."
} else {
    Write-Output "WinRAR is not installed."
}
